#include <iostream>

//sheet_3 pb_6
//A Program That Prints The Sum of Numbers from 1 to 100 That is Divisible By 2 & 3

using namespace std;

int main()
{
    int intial = 1;
    int last = 100;
    for(int i = intial; i <= last; i++)
    {
        if(i % 2 == 0)
        {
            if(i % 3 == 0)
                cout<< i << endl;
        }
    }


    return 0;

}
