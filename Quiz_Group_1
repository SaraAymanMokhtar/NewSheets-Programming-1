#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n;
    cout << "enter the number of rows" << endl;
    cin>> n;

    //prints the number of rows
    for(int i = 0; i < n; i++)
    {
        int distance = abs(i - n / 2);
        int maximum = ceil(n / 2.0);
        
        for(int j = 0; j < maximum - distance; j ++)
        {
            cout << j + 1;
        }
        cout<<endl;
    }
    return 0;
}
