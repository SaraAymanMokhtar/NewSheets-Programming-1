#include <iostream>
#include <cmath>

//sheet_3 pb_3d
//A Program That Calculate The Value of A Series

using namespace std;

int main()
{
    int n;
    int output = 0, k = 5;
    cin>> n;
    for(int i = 1 ; i <= n; i++)
    {
        output += (k * i);
    }
    cout<< output <<endl;
    return 0 ;

}
