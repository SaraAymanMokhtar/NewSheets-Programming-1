#include <iostream>
#include <cmath>

//sheet_3 pb_3e
//A Program That Calculate The Value of A Series

using namespace std;

int main()
{
    float n;
    float output = 0, A = 5, B = 10;
    cin>> n;
    for(float i = 0 ; i < n; i++)
    {
        output += (1 / (A + i * B));
    }
    cout<< output <<endl;
    return 0 ;

}
