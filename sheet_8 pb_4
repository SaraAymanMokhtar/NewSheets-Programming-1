#include <iostream>


using namespace std;


// sheet 8_Q4
//write a function to print a matrix of size N * M
int main()
{
    int n =10;
    int m = 4;
    int array[n][m];
    for(int i = 0 ; i < n ; i++)
    {
        for (int j = 0 ; j < m ; j++)
            cout << "*";
        cout << endl;
    }

    return 0;
}

