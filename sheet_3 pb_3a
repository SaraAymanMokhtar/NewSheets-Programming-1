#include <iostream>
#include <cmath>

//sheet_3 pb_3a
//A Program That Calculate The Value of A Series

using namespace std;

int main()
{
    int n;
    int output = 0;
    cin>> n;
    for(int i = 1 ; i <= n; i++)
    {
        output += pow(i , 2);
    }
    cout<<output<<endl;
    return 0 ;

}
